{
  "name":"RCGAN",
  "is_train": true,
  "rgb_range": 1,
  "datasets": {
    "train": {
      "dataroot_VI": "./Demo_dataset/train/sub_img/vi",
      "dataroot_IR": "./Demo_dataset/train/sub_img/ir",
      "dataroot_PF": "./Demo_dataset/train/sub_img/pf",
      "data_type": "img",
      "n_workers": 6,
      "batch_size": 32,
      "Image_Size": 132,
      "Label_Size": 120,
      "use_flip": true
    },
    "val": {
      "dataroot_VI": "./Demo_dataset/val/vi",
      "dataroot_IR": "./Demo_dataset/val/ir",
      "dataroot_PF": "./Demo_dataset/val/pf",
      "data_type": "img"
    }
  },
  "networks": {
    "G": {
    "which_model": "CoGAN_G",
    "norm_type": "bn",
    "mode": "NAC",
    "num_features": 256,
    "in_channels": 1,
    "out_channels": 1
    },
    "D": {
    "which_model": "CoGAN_D",
    "norm_type": "bn",
    "act_type": "lrelu",
    "mode": "CNA",
    "num_features": 64,
    "in_channels": 1
    }
  },
  "train": {
    "type": "ADAM",
    "lr_G": 1e-4,
    "weight_decay_G": 0,
    "beta1_G": 0.9,
    "lr_D": 1e-4,
    "weight_decay_D": 0,
    "beta1_D": 0.9,
    "lr_gamma": 0.5,
    "G_step": 1,
    "D_step": 1,
    "pixel_criterion": "l2",
    "pixel_weight": 1.0,
    "feature_criterion": "l2",
    "feature_weight": 0.5,
    "gan_type": "lsgan",
    "gan_weight": 1,
    "lambda": 100,
    "epsilon": 6,
    "D_update_ratio": 1,
    "D_init_iters": 0,
    "gp_weigth": 10,
    "manual_seed": 0,
    "num_epochs": 10,
    "log_step": 1,
    "val_step": 1,
    "resume": true,
    "resume_path": "./experiments/COGAN_G_COGAN_D_p1f0_x"
  }
}
